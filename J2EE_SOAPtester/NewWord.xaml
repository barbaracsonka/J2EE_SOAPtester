<Window x:Class="J2EE_SOAPtester.NewWord"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:J2EE_SOAPtester"
        mc:Ignorable="d"
        Title="NewWord" Height="217.597" Width="400">
    <Grid>
        <StackPanel>
            <Label Background="LightBlue" Content="Original" />
            <TextBox x:Name="tb_original" Margin="10"/>
            <Label Background="LightBlue" Content="Translated" />
            <TextBox x:Name="tb_translated" Margin="10"/>
            <Button Content="Add" Margin="10" Click="Button_Click" />
        </StackPanel>
    </Grid>
</Window>
